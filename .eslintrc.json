{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console":0,
        "prefer-const": ["error", {
            "destructuring": "any",
            "ignoreReadBeforeAssign": false
        }],
        "quotes": [1, "single", "avoid-escape"],
        "semi": [1, "always"],
        "indent": ["error", 4, { "SwitchCase": 2 }],
        "no-var": 1,
        "max-len": ["error", 150],
        "no-underscore-dangle": 0,
        "no-param-reassign": 0
    }
}
